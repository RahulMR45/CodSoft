    "import random\n",
    "import string\n",
    "\n",
    "\n",
    "def password(l):\n",
    "    c = string.ascii_letters + string.digits + string.punctuation\n",
    "    p = \"\"\n",
    "    for x in range(l):\n",
    "        p += random.choice(c)\n",
    "    print(p)\n",
    "\n",
    "\n",
    "l = int(input(\"Enter the number of characters for the password: \"))\n",
    "password(l)"
   ]
